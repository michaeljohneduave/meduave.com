---
import Footer from "../components/Footer.astro";
import Header from "../components/Header.astro";
import Links from "../components/Links.astro";
import Projects from "../components/ProjectList.astro";
import Layout from "../layouts/Layout.astro";

export const prerender = true;
export const name = "Michael Eduave";
export const description = "Software Engineer";
---

<script>
  fetch("/onlyfans?t=true");
</script>
<Layout title={name} description={description}>
  <Header name={name} description={description} />
  <div class="flex flex-col pt-5 gap-y-5 grow">
    <p>
      Hi, I'm Michael, and I'm passionate about writing high-quality code. I
      believe in continuous improvement (1.01<sup>365</sup>) , always seeking
      new knowledge and better ways to implement it. I'm driven by the
      opportunity to work on and build products that make a positive impact on
      people and the world.
    </p>
    <Links />
    <Projects />
    <Footer />
  </div>
</Layout>
